{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [true, "never-prefix"],
        "max-line-length": [true, 80],
        "no-any": true,
        "no-console": [true],
        "only-arrow-functions": [true],
        "quotemark": [true, "single"],
        "semicolon": [false]
    },
    "rulesDirectory": []
}
